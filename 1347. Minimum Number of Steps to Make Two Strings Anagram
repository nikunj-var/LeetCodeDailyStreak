class Solution {
public:
    int minSteps(string s, string t) {
        int count=0;
        unordered_map<char,int> mp;
        for(int i = 0;i<s.length();i++){
            mp[s[i]]++;
        }
        for(int i = 0;i<t.length();i++){
            if(mp[t[i]]){
                mp[t[i]]--;
            }
        }
        for(auto i :mp){
            if(i.second > 0){
                count+=i.second;
            }
        }
        return count;
    }
};
